{
    "name": "blog-api",
    "version": "1.4.0",
    "author": "Sam Pastoriza [pastorsj](https://github.com/pastorsj)",
    "repository": "https://github.com/pastorsj/blog-api",
    "private": false,
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pastorsj/blog-api/issues"
    },
    "homepage": "https://github.com/pastorsj/blog-api#readme",
    "description": "A simple express-based api to serve a blog",
    "scripts": {
        "start": "babel-node ./src/server.js",
        "https": "babel-node ./src/server.js --type=https",
        "http": "babel-node ./src/server.js --type=http",
        "watch": "nodemon --exec babel-node -- ./src/server.js",
        "build": "webpack --env build --display-error-details --optimize-minimize --mode production",
        "dev": "webpack --progress --colors --env dev --display-error-details --mode development",
        "lint": "eslint src/* tests/api/* tests/business/*",
        "autofix": "eslint --fix src/* tests/api/* tests/business/*",
        "docker": "docker-compose -f docker-compose.yml up --build --force-recreate",
        "test": "NODE_ENV=TEST mocha --require @babel/register --colors --timeout 5000 --exit ./tests/**/*.test.js",
        "test:watch": "NODE_ENV=TEST mocha --require @babel/register --colors --timeout 5000 --exit -w ./tests/**/*.test.js",
        "coverage": "NODE_ENV=TEST istanbul cover node_modules/mocha/bin/_mocha -- --require @babel/register --colors --timeout 5000 --exit --reporter dot ./tests/**/*.test.js",
        "coverage:coveralls": "NODE_ENV=TEST istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --require @babel/register --colors --timeout 5000 --exit --reporter spec ./tests/**/*.test.js"
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/runtime": "^7.3.4",
        "aws-sdk": "^2.413.0",
        "babel-loader": "^8.0.5",
        "body-parser": "~1.18.3",
        "compression": "^1.7.3",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "express": "~4.16.4",
        "express-graphql": "^0.7.1",
        "express-jwt": "^5.3.1",
        "express-limiter": "^1.6.1",
        "express-session": "^1.15.6",
        "express-winston": "^2.5.1",
        "gm": "^1.23.1",
        "graphql": "^14.1.1",
        "helmet": "^3.15.1",
        "jsonwebtoken": "^8.5.0",
        "method-override": "^3.0.0",
        "mime": "^2.4.0",
        "mongoose": "^5.4.16",
        "mongoose-plugin-autoinc": "^1.1.9",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "node-gist-html": "^1.1.4",
        "nodemon": "^1.18.10",
        "readline": "^1.3.0",
        "redis": "^2.8.0",
        "serve-favicon": "~2.5.0",
        "spdy": "^4.0.0",
        "web-push": "^3.3.3",
        "winston": "^3.1.0",
        "wysiwyg-editor-node-sdk": "git+https://github.com/froala/wysiwyg-editor-node-sdk.git",
        "yargs": "^13.2.1"
    },
    "devDependencies": {
        "@babel/node": "^7.2.2",
        "@babel/register": "^7.0.0",
        "chai": "^4.2.0",
        "coveralls": "^3.0.3",
        "eslint": "^6.2.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.16.0",
        "istanbul": "^1.1.0-alpha.1",
        "mocha": "^6.0.2",
        "sinon": "^7.2.6",
        "sinon-chai": "^3.3.0",
        "supertest": "^3.4.2",
        "webpack": "^4.19.6",
        "webpack-cli": "^3.2.3",
        "webpack-node-externals": "^1.7.2"
    }
}
