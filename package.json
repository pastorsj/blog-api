{
  "name": "blog-api",
  "version": "1.1.0",
  "author": "Sam Pastoriza [pastorsj](https://github.com/pastorsj)",
  "repository": "https://github.com/pastorsj/blog-api",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pastorsj/blog-api/issues"
  },
  "homepage": "https://github.com/pastorsj/blog-api#readme",
  "description": "A simple express-based api to serve a blog",
  "scripts": {
    "start": "babel-node ./src/server.js",
    "https": "babel-node ./src/server.js --type=https",
    "http": "babel-node ./src/server.js --type=http",
    "build": "webpack --env build --display-error-details",
    "dev": "webpack --progress --colors --env dev --display-error-details",
    "lint": "eslint src/* test/*",
    "autofix": "eslint --fix src/* test/*",
    "test": "NODE_ENV=TEST mocha --require babel-register --colors --timeout 5000 --exit ./tests/**/*.test.js",
    "test:watch": "NODE_ENV=TEST mocha --require babel-register --colors --timeout 5000 --exit -w ./tests/**/*.test.js",
    "coverage": "NODE_ENV=TEST istanbul cover node_modules/mocha/bin/_mocha -- --require babel-register --colors --timeout 5000 --exit --reporter dot ./tests/**/*.test.js",
    "coverage:coveralls": "NODE_ENV=TEST istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --require babel-register --colors --timeout 5000 --exit --reporter spec ./tests/**/*.test.js"
  },
  "dependencies": {
    "aws-sdk": "^2.114.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "~1.18.1",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "express": "~4.15.4",
    "express-jwt": "^5.3.0",
    "express-limiter": "^1.6.1",
    "express-session": "^1.15.5",
    "express-winston": "^2.4.0",
    "helmet": "^3.8.1",
    "jsonwebtoken": "^8.0.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mime": "^2.0.2",
    "mongoose": "^4.12.4",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.3.0",
    "node-gist-html": "^1.1.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "readline": "^1.3.0",
    "redis": "^2.8.0",
    "serve-favicon": "~2.4.4",
    "spdy": "^3.4.7",
    "transform-runtime": "0.0.0",
    "winston": "^2.4.0",
    "wysiwyg-editor-node-sdk": "git+https://github.com/froala/wysiwyg-editor-node-sdk.git",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  }
}
